{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended", "plugin:security/recommended"],
  "plugins": ["prettier", "jest", "security"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "jest/globals": true
  }
}
  